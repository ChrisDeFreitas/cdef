{

  "manifest_version": 2,
  "name": "cdef",
  "description": "Displays a sidebar that lets you take notes on web pages.",
  "version": "1.0",


  "background": {
    "scripts":["lib/videoDl.js","lib/CMessageQue.js","lib/ui.js"]
  },

  "browser_action": {
    "default_area": "navbar",
    "default_icon": "icons/donut_small-black-18dp.svg",
    "default_title": "cdef",
    "default_popup": "sidebar/sidebar.html"
  },
  "browser_specific_settings": {
    "gecko": {
      "id": "cdef@example.com",
      "strict_min_version": "54.0a1"
    }
  },
  "commands": {
    "_execute_sidebar_action": {
        "suggested_key": {
          "default": "Ctrl+Shift+C"
        }
    }
  },
  "icons": {
    "16": "icons/donut_small-black-18dp.svg",
    "48": "icons/donut_small-black-18dp.svg",
    "96": "icons/donut_small-black-18dp.svg"
  },
  "permissions": ["downloads","storage", "tabs"],
  "sidebar_action": {
    "default_icon": "icons/donut_small-black-18dp.svg",
    "default_title" : "cdef",
    "default_panel": "sidebar/sidebar.html",
    "open_at_install":true
  }

}